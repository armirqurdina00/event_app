    "@typescript-eslint/parser": "^5.8.1",
    "chai": "^4.3.10",
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.7",
    "playwright": "^1.38.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "check-linter": "eslint --ext .ts src",
    "fix-linter": "eslint --fix --ext .ts src",
    "check-prettier": "npx prettier --ignore-path ../.gitignore --check .",
    "fix-prettier": "npx prettier --ignore-path ../.gitignore --write .",
    "fix": "npm run fix-linter && npm run fix-prettier",
    "oaslint": "spectral dotenv -elint ../oas/*",
    "clean": "rm -rf node_modules && rm -rf build && rm -rf .nyc_output && rm -rf coverage && rm -rf src/helpers-for-tests/backend_client",
    "rm-pics": "rm -f src/crawling-and-scraping/scraper/*.png",
    "build": "npx tsoa spec-and-routes && npx openapi-typescript-codegen --client axios --input ../backend/build/swagger.json --output src/helpers-for-tests/backend_client --name BackendClient && tsc && cp src/helpers/ca-certificate.crt build/src/helpers",
    "test-api": "tsoa spec-and-routes && npx openapi-typescript-codegen --client axios --input ../backend/build/swagger.json --output src/helpers-for-tests/backend_client --name BackendClient && mocha -r ts-node/register src/**/*.spec.ts --bail",
    "test-scraper": "mocha -r ts-node/register src/crawling-and-scraping/scraper/FacebookScraper.spec.ts --bail",
    "test-crawler": "mocha -r ts-node/register src/crawling-and-scraping/crawler/FacebookCrawler.spec.ts --bail",
    "test-urlmanager": "mocha -r ts-node/register src/crawling-and-scraping/crawler/ScrapeUrlManager.spec.ts --bail",
    "test-scheduler": "mocha -r ts-node/register src/crawling-and-scraping/scheduler/Scheduler.spec.ts --bail",
    "test": "npm run test-api && npm run test-scraper && npm run test-crawler && npm run test-urlmanager",
    "add_groups": "tsoa spec-and-routes && npx openapi-typescript-codegen --client axios --input ../backend/build/swagger.json --output src/helpers-for-tests/backend_client --name BackendClient && mocha -r ts-node/register src/tests/AddGroups.ts --bail",
    "add_events": "tsoa spec-and-routes && npx openapi-typescript-codegen --client axios --input ../backend/build/swagger.json --output src/helpers-for-tests/backend_client --name BackendClient && mocha -r ts-node/register src/tests/AddEvents.ts --bail",
    "coverage": "nyc npm run test",
    "dev": "dotenv -e .env/.dev_env npx nodemon",
    "prod": "dotenv -e .env/.prod_env npx nodemon",
    "debug": "dotenv -e .env/.dev_env ts-node src/debug.ts",
    "crawl-dev": "dotenv -e .env/.dev_env ts-node src/crawling-and-scraping/run.ts",
    "crawl": "npm run rm-pics && dotenv -e .env/.prod_env ts-node src/crawling-and-scraping/run.ts",
    "crawl-cron-dev": "npm run rm-pics && dotenv -e .env/.dev_env ts-node src/crawling-and-scraping/run-as-cron-job.ts",
    "crawl-cron": "npm run rm-pics && dotenv -e .env/.prod_env ts-node src/crawling-and-scraping/run-as-cron-job.ts",
    "start": "node build/src/index.js"
  },
  "author": "Magnus GÃ¶dde",
  "license": "No licence",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "bail": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "common.ts"
    ]
  }
}
