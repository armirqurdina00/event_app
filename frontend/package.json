{
  "name": "next-pwa-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf utils/backend_client; rm -rf node_modules; rm -rf AccessToken.tmp",
    "check-linter": "eslint --ext .ts,.tsx .",
    "fix-linter": "eslint --fix --ext .ts,.tsx .",
    "check-prettier": "npx prettier --ignore-path ../.gitignore --check .",
    "fix-prettier": "npx prettier --ignore-path ../.gitignore --write .",
    "fix": "npm run fix-linter && npm run fix-prettier",
    "dev": "next dev",
    "build_backend_client": "npx openapi-typescript-codegen --client fetch --input ../backend/build/swagger.json --output utils/backend_client --name BackendClient",
    "build": "npx openapi-typescript-codegen --client fetch --input ../backend/build/swagger.json --output utils/backend_client --name BackendClient && next build",
    "start": "next start",
    "test": "npx playwright test"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "@mui/x-date-pickers": "^6.10.2",
    "@types/formidable": "^3.4.0",
    "@types/googlemaps": "^3.43.3",
    "@types/react": "^18.2.20",
    "add-to-calendar-button-react": "^2.4.3",
    "axios": "^1.5.0",
    "cookie": "^0.5.0",
    "cookies-next": "^2.1.2",
    "form-data": "^4.0.0",
    "formidable": "^3.5.0",
    "fs": "^0.0.1-security",
    "i18next": "^23.7.7",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "mui-file-input": "^3.0.0",
    "next": "^13.4.12",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^15.0.0",
    "next-pwa": "^5.5.2",
    "next-themes": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-share": "^4.4.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@next/eslint-plugin-next": "^14.0.1",
    "@playwright/test": "^1.37.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.52.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "openapi-typescript-codegen": "^0.25.0",
    "pg": "^8.11.3",
    "playwright": "^1.37.0",
    "postcss": "^8.4.14",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
    "tailwindcss-safe-area": "^0.2.2",
    "typeorm": "^0.3.17",
    "typescript": "^4.6.4"
  }
}
